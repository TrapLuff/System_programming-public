mkdir Work
cd ./Work
mkdir "Лабораторная работа №1!"
mkdir "Тенишев Александр Амирович"
chmod 772 "Лабораторная работа №1!"
chmod 200 "Тенишев Александр Амирович"
touch 'Text @1'
touch 'Text $2'
touch 'Text #3'
printf "Птица говорун отличается умом и сообразительностью! %s\nОтличается умом, отличается сообразительностью..." > 'Text @1'
cat 'Text @1' > 'Text $2'
mv 'Text $2' 'Лабораторная работа №1!'
chmod 700 "Тенишев Александр Амирович"
cp 'Text @1' 'Тенишев Александр Амирович'
chmod 200 "Тенишев Александр Амирович"
gzip 'Text @1' 'Text #3'
tar -cf 'MyArchiv' 'Text @1.gz' 'Text #3.gz'
gzip -d 'Text @1.gz' 'Text #3.gz'
head -n 1 'Text @1' > 'Text #3'
echo "Будь осторожен! Преступник вооружен!" >> 'Text #3'
rev 'Text @1' > ./'Тенишев Александр Амирович'/'Result_one'
gzip 'Text @1' 'Text #3'
mv 'Тенишев Александр Амирович'/'Result_one' 'Тенишев Александр Амирович'/'Result_two'
touch 'Лабораторная работа №1!'/Result_3
for f in 'Тенишев Александр Амирович'/T*; do
  if [ -f "$f" ]; then
    cat "$f" >> ./'Лабораторная работа №1!'/Result_3
  fi
done
echo -e "\n" >> ./'Лабораторная работа №1!'/Result_3
uname -a >> 'Лабораторная работа №1!'/Result_3
date >> 'Лабораторная работа №1!'/Result_3